each wrappedLevelParamsets: tr
  if showVaccine
    td rowspan=numberOfLevels
      .btn-group
        button.btn.btn-default type="button": view.requirement.vaccine.initials
        button.btn.btn-default type="button": view.requirement.vaccine.doses_per_vial
  with paramset
    th: level.name
    td: Ember.Select content=view.temperatures optionLabelPath="content.label" optionValuePath="content.id" value=temperature prompt="Temperatures ..." class="form-control"
    td: Ember.Select content=view.packings optionLabelPath="content.label" optionValuePath="content.id" value=packing class="form-control" prompt="Packings ..."
    td: = input type="checkbox" class="form-control" checked=warm_diluent
    td: = input type="text" class="form-control" value=safety_stock
    td: = input type="text" class="form-control" value=supply_interval
    td: rounded-number number=reorder_freq
    td: rounded-number number=storage_volume_per_1000
